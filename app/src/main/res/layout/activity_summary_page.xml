<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SummaryPage">

    <TextView
        android:id="@+id/tvHeading2"
        android:layout_width="329dp"
        android:layout_height="60dp"
        android:fontFamily="@font/ostrichsans_black"
        android:text="Run Summary"
        android:textAlignment="center"
        android:textSize="55sp"
        android:textColor="#010101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065" />

    <TextView
        android:id="@+id/lblDate"
        android:layout_width="255dp"
        android:layout_height="43dp"
        android:fontFamily="@font/ostrich_regular"
        android:text="Date:"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="40dp"
        android:textColor="#1942EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.202" />

    <TextView
        android:id="@+id/tvDate"
        android:layout_width="258dp"
        android:layout_height="44dp"
        android:fontFamily="@font/ostrichsans_black"
        android:text=""
        android:textAlignment="center"
        android:textSize="25sp"
        android:textColor="#010101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.285" />

    <TextView
        android:id="@+id/lblMetres"
        android:layout_width="255dp"
        android:layout_height="46dp"
        android:fontFamily="@font/ostrich_regular"
        android:text="Metres Run:"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="40dp"
        android:textColor="#1942EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.383" />

    <TextView
        android:id="@+id/tvMetresRun"
        android:layout_width="258dp"
        android:layout_height="44dp"
        android:fontFamily="@font/ostrichsans_black"
        android:text=""
        android:textAlignment="center"
        android:textSize="25sp"
        android:textColor="#010101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.473" />

    <TextView
        android:id="@+id/lblCalories"
        android:layout_width="258dp"
        android:layout_height="42dp"
        android:fontFamily="@font/ostrich_regular"
        android:text="Calories Burned:"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="40dp"
        android:textColor="#1942EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.564" />

    <TextView
        android:id="@+id/tvCalories"
        android:layout_width="258dp"
        android:layout_height="44dp"
        android:fontFamily="@font/ostrichsans_black"
        android:text=""
        android:textAlignment="center"
        android:textSize="25sp"
        android:textColor="#010101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.66" />

    <TextView
        android:id="@+id/lblTimeRan"
        android:layout_width="263dp"
        android:layout_height="46dp"
        android:fontFamily="@font/ostrich_regular"
        android:text="Time Ran (in Seconds):"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textSize="40dp"
        android:textColor="#1942EA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.76" />

    <TextView
        android:id="@+id/tvTimeRan"
        android:layout_width="258dp"
        android:layout_height="44dp"
        android:fontFamily="@font/ostrichsans_black"
        android:text=""
        android:textAlignment="center"
        android:textSize="25sp"
        android:textColor="#010101"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.848" />

    <Button
        android:id="@+id/btnReturn"
        android:layout_width="163dp"
        android:layout_height="48dp"
        android:background="@drawable/rounded_corners"
        android:fontFamily="@font/ostrich_regular"
        android:onClick="doExit"
        android:text="Return to Run"
        android:textColor="#FFF"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

</androidx.constraintlayout.widget.ConstraintLayout>